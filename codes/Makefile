CLINK=-L../iLayout/ -L/usr/local/lib -llept -L/etc/gtk-2.0/i386-redhat-linux-gnu/ -lpthread -lpng -ljpeg -ltiff #liblept.a -L/usr/lib/i386-linux-gnu/

all:	$(OBJ)
	$(CC) -I/usr/local/include/leptonica/  -lm $(CCFLAGS) $(OBJ) $(CINCLUDE) $(CLINK) -o $(OUTPUT) -Wno-deprecated

%.o:	%.cpp
	$(CC) $(CCFLAGS) $(CINCLUDE) -I/usr/local/include/leptonica/  -c $<

%.o:	%.c
	$(CC) $(CCFLAGS) $(CINCLUDE) -I/usr/local/include/leptonica/  -c $<

clean:
	rm $(OBJ) $(OUTPUT) 
